@keyframes ripple{
  0% {
    box-shadow: 0 0 0 0 rgba(254,101,101, 0.3),
                0 0 0 30rpx rgba(254,101,101, 0.3),
                0 0 0 60rpx rgba(254,101,101, 0.3),
                0 0 0 90rpx rgba(254,101,101, 0.3);
  }
  50% {
    box-shadow: 0 0 0 30rpx rgba(254,101,101, 0.3),
                0 0 0 60rpx rgba(254,101,101, 0.3),
                0 0 0 90rpx rgba(254,101,101, 0.3),
                0 0 0 120rpx rgba(254,101,101, 0.3,);
  }
  100% {
    box-shadow: 0 0 0 0rpx rgba(254,101,101, 0.3),
                0 0 0 20rpx rgba(254,101,101, 0.3),
                0 0 0 50rpx rgba(254,101,101, 0.3),
                0 0 0 80rpx rgba(254,101,101, 0.3,);
  }
}
@keyframes ripple1{
	0%{
		box-shadow: 0 0 0 10rpx rgba(254,101,101, 0.2);
		transform:scale3d(1,1,1);
	}
	50%{
		box-shadow: 0 0 0 20rpx rgba(254,101,101, 0.2);
		transform:scale3d(1.1,1.1,1.1);
	}
	100%{
		box-shadow: 0 0 0 10rpx rgba(254,101,101, 0.2);
		transform:scale3d(0.8,0.8,0.8);
	}
}
@keyframes ripple2{
	0%{
		box-shadow: 0 0 0 10rpx rgba(254,101,101, 0.2);
		transform:scale3d(1,1,1);
	}
	50%{
		box-shadow: 0 0 0 32rpx rgba(254,101,101, 0.2);
		transform:scale3d(1.1,1.1,1.1);
	}
	100%{
		box-shadow: 0 0 0 10rpx rgba(254,101,101, 0.2);
		transform:scale3d(0.8,0.8,0.8);
	}
}
@keyframes ripple3{
	0%{
		box-shadow: 0 0 0 4rpx rgba(254,101,101, 0.3);
		transform:scale3d(1,1,1);
	}
	50%{
		box-shadow: 0 0 0 16rpx rgba(254,101,101, 0.3);
		transform:scale3d(1.1,1.1,1.1);
	}
	100%{
		box-shadow: 0 0 0 4rpx rgba(254,101,101, 0.3);
		transform:scale3d(1,1,1);
	}
}
/*********************小心心***********************/
@keyframes beat {
    0% {transform: scale(0.8,0.8);}
    50% {transform: scale(1,1);}
    100% {transform: scale(0.7,0.7);}
}