/********************circle动效*********************/
.circle-ripple {
  background-color: #fe6565;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  transition: all 1s;
  animation: ripple 0.8s linear infinite;
}
.ripple{
  text-decoration: none;
  color: #fff;
  width: 12px;
  height: 12px;
  background-color: rgba(254,101,101);
  margin: 0 auto;
  border-radius: 100px;
  transition: all 2s;
  -webkit-animation: ripple .7s linear infinite;
  animation: ripple .7s linear infinite;
}
