/**index.wxss**/

@import "../../wxParse/wxParse.wxss"
.submit_info{
	width:468rpx;
	height:76rpx;
	border-radius:50rpx;
	line-height:76rpx;
	text-align:center;
	margin:66rpx auto;
}
.things_img{
	width:648rpx;
	height:442rpx;
	margin:0 auto;
	border-radius: 20rpx;
	box-shadow: 0 0 6rpx #000;
	overflow: hidden;
}
.things_img image{
	width:100%;
	height: 100%;
}
.things_time{
	height:116rpx;
	margin-top: 60rpx;
}
.things_tlt{
	width:26%;
	text-align: center;
}
.things_right{
	width:74%;
	border-bottom:solid 1px #414141;
	padding-bottom: 10rpx;
}
.thing1{
	width:20%;
	height: 44rpx;
	background: #fe6666;
	border-radius: 50rpx;
	padding:0 2%;
	line-height: 44rpx;
}
.years{
	width:6%;
}
.thing1 image{
	width:18rpx;
	height:18rpx;
}
.thing1>view{
	width:70%;
}
.uplode{
	width:256rpx;
	height:256rpx;
	margin:0 auto;
}
.uplode image{
	width:100%;
	height:100%;
}
.confirm{
	width:100%;
	height: 90rpx;
	border-radius: 20rpx 20rpx 0 0;
	text-align: center;
	line-height:90rpx;
	position:fixed;
	left: 0;
	bottom: 0;
	overflow:hidden;
}
.confirm button{
	background: #fe6565;
	width:100%;
	height:100%;
	border-radius: 0;
}